 // 1. მოცემულია 2 ცვლადი. გავუცვალოთ ადგილები. (არ გამოვიყენოთ დამატებითი ცვლადი)
 
 {
    int a;
    int b;
  
    a = 2;
    b = 4;
  
    a = (a * b) / a;
    b = (a * b) / b;
 }
 
 
 /*
 2. ვიპოვოთ მარკუთხედის ფართობი. მოცემულია 2 მთელი დადებითი რიცხვი a და b. 
    ავღწეროთ ცვლადი სახელად area და მივანიჭოთ მართკუთხედის ფართობი.
 */
 
 {
     int height;
     int width;
     int area;
     
     height = 10;
     width = 20;
     
     area = height * width
 }
    
    
//  3. პირობა იგივეა რაც მეორე ამოცანაში ოღონდ ვიპოვოთ პერიმეტრი
    
    {
      int height;
      int width;
      int P;
      
      height = 10;
      width = 20;
      
      P = (height + width) * 2;
    }
    
    

  
  
  
  
        
        
        
        
        
        
        
